	<name>SOLR</name>
	<classname>com.xerox.docushare.elf.index.server.solr.SolrIndexer</classname>
	<collectionName>Falcon</collectionName>
	<host>127.0.0.1</host>
	<port>8983</port>
	<useTika>true</useTika>
    </Properties>
  </Indexer>
