    <numberOfIndexingThreads>4</numberOfIndexingThreads>
    <numberOfDataThreads>4</numberOfDataThreads>
    <enableContentIndexing>true</enableContentIndexing>
    <timeout>60</timeout>
    <verbose>false</verbose>
    <timing>true</timing>
    <maxIndexRetry>10</maxIndexRetry>
